<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>A simple Email Sender with Razor email templates support for .NET Core.</Description>
    <Copyright>Copyright Linco Ñanco</Copyright>
    <AssemblyName>MailingFerret</AssemblyName>
    <PackageId>MailingFerret</PackageId>
    <PackageTags>emails;razortemplates;</PackageTags>
    <!-- <PackageIconUrl>https://raw.githubusercontent.com/jbogard/MediatR/master/assets/logo/gradient_128x128.png</PackageIconUrl> -->
    <PackageProjectUrl>https://github.com/LincoNanco/MailingFerret</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/LincoNanco/MailingFerret/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com/LincoNanco/MailingFerret</RepositoryUrl>
    <PackageVersion>1.0.5</PackageVersion>
    <Authors>Linco Ñanco</Authors>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore" Version="2.2.0"/>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0"/>
  </ItemGroup>
</Project>